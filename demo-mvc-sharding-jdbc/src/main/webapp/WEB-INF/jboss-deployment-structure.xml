<?xml version="1.0"?>
<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.3">
    <ear-subdeployments-isolated>false</ear-subdeployments-isolated>
    <deployment>
        <exclusions>
            <module name="org.apache.commons.logging"></module>
            <module name="org.apache.httpcomponents"></module>
            <module name="org.slf4j"></module>
            <module name="org.slf4j.ext"></module>
            <module name="org.slf4j.impl"></module>
            <module name="org.apache.log4j"></module>
            <module name="org.jboss.resteasy.resteasy-jackson-provider"></module>
            <module name="com.fasterxml.jackson.core.jackson-databind"></module>
            <module name="com.fasterxml.jackson.core.jackson-core"></module>
            <module name="com.fasterxml.jackson.core.jackson-annotations"></module>
            <module name="com.fasterxml.jackson.jaxrs.jackson-jaxrs-json-provider"></module>
            <module name="org.jboss.resteasy.resteasy-jackson2-provider"></module>
        </exclusions>
        <dependencies>
            <module name="org.jboss.ironjacamar.jdbcadapters"></module>
        </dependencies>
    </deployment>
</jboss-deployment-structure>
